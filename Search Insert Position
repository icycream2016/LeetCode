class Solution(object):
    def searchInsert(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        用二分法查找插入位置
        需要注意循环的条件：i<=j。
        每次修改左、右指针时：左指针=中间+1，右指针=中间-1。
        这样可以应对插入值超过最小或最大时死循环的问题。
        """
        i=0
        j=len(nums)-1
        while(i<=j):
            m=(i+j)/2
            if(target<=nums[m]):
                j=m-1
            else:
                i=m+1
        return i
